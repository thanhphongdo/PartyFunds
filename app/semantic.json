{
  "base": "semantic\\",
  "paths": {
    "source": {
      "config": "src/theme.config",
      "definitions": "src/definitions/",
      "site": "src/site/",
      "themes": "src/themes/"
    },
    "output": {
      "packaged": "..\\assets\\semantic\\dist\\",
      "uncompressed": "..\\assets\\semantic\\dist\\components\\",
      "compressed": "..\\assets\\semantic\\dist\\components\\",
      "themes": "..\\assets\\semantic\\dist\\themes\\"
    },
    "clean": "dist/"
  },
  "permission": false,
  "autoInstall": false,
  "rtl": false,
  "components": [
    "reset",
    "site",
    "button",
    "icon",
    "image",
    "input",
    "label",
    "loader",
    "form",
    "grid",
    "menu",
    "message",
    "table",
    "accordion",
    "checkbox",
    "dropdown",
    "modal",
    "search",
    "sidebar",
    "tab",
    "transition",
    "form"
  ],
  "version": "2.4.2"
}